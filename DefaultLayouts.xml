<?xml version="1.0" encoding="utf-8"?>
<FullDefaultLayoutTemplate 
    xmlns="http://schemas.microsoft.com/Start/2014/FullDefaultLayout"
    xmlns:start="http://schemas.microsoft.com/Start/2014/StartLayout"
    Version="1">
